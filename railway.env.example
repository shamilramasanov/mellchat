# Railway Configuration
# Замените эти значения на ваши данные из Railway

# PostgreSQL Database
DATABASE_URL=postgresql://username:password@host:port/database

# Redis (уже настроен)
REDIS_URL=redis://username:password@host:port

# Другие настройки
NODE_ENV=production
PORT=3001

